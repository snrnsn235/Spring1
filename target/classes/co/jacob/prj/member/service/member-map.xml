<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.jacob.prg.member.service.MemberService">
	<select id="memberSelectList" resultType="co.jacob.prg.member.service.MemberVO">
		SELECT * FROM MEMBER
	</select>
	<select id = "memberSelect" resultType="co.jacob.prg.member.service.MemberVO">
		SELECT * FROM MEMBER WHERE ID = ${id}
	</select>
	
	<insert id="memberInsert"></insert>
	<update id="memberUpdate"></update>
	<delete id="memberDelete"></delete>
</mapper>